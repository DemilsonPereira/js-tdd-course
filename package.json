{
  "name": "js-tdd-course",
  "version": "1.0.0",
  "description": "Nossa biblioteca do curso de JS com TDD na pr√°tica",
  "main": "index.js",
  "scripts": {
    "lint": "./node_modules/.bin/eslint src/*.js",
    "prepush": "npm run lint && npm run test:coverage",
    "test": "mocha './tests/**/*.spec.js' --require babel-register",
    "test:tdd": "npm run test -- --watch",
    "test:coverage": "nyc --reporter=lcov --reporter=text npm run test"
  },
  "nyc": {
    "functions": 80,
    "lines": 80,
    "check-coverage": true,
    "reporter": ["text", "html"],
    "exclude": ["tests/**"]
  },
  "repository": {
    "type": "git",
    "url": "(\u0016\u0016\u0016\u0016https://github.com/DemilsonPereira/js-tdd-course.git)"
  },
  "keywords": [
    "js",
    "tdd",
    "library"
  ],
  "author": "Demilson Pereira <demilson.d2102@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "mocha": "^7.1.0",
    "nock": "^12.0.2",
    "nyc": "^15.0.0",
    "sinon": "^9.0.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jest": "^24.3.6",
    "husky": "0.11.9",
    "node-fetch": "^2.1.2",
    "rimraf": "^2.6.2"
  }
}
